<%=form_with(model:book_review) do |form|%>
    <div>
        <%=form.label :title, style: "display:block"%>
         <%=form.text_field :title %>
    </div>
    <div>
        <%=form.label :body, style: "display:block"%>
         <%=form.text_area :body %>
    </div>
    <%=form.submit %>
<%end%>

